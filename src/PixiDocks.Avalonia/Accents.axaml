<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <ResourceDictionary.ThemeDictionaries>
        <ResourceDictionary x:Key="Dark">
            <Color x:Key="ThemeDockAccentColor">#b00022</Color>
            <Color x:Key="ThemeDockHighlightColor">#4b4b4b</Color>
            <Color x:Key="ThemeDockBackgroundLowColor">#252525</Color>
            <Color x:Key="ThemeDockForeground">#fff</Color>
            <CornerRadius x:Key="ThemeDockTabStripCornerRadius">5,5,0,0</CornerRadius>
            <SolidColorBrush x:Key="ThemeDockAccentBrush" Color="{DynamicResource ThemeDockAccentColor}" />
            <SolidColorBrush x:Key="ThemeDockHighlightBrush" Color="{DynamicResource ThemeDockHighlightColor}" />
            <SolidColorBrush x:Key="ThemeDockForegroundBrush" Color="{DynamicResource ThemeDockForeground}" />
            <SolidColorBrush x:Key="ThemeDockBackgroundLowBrush" Color="{DynamicResource ThemeDockBackgroundLowColor}" />
        </ResourceDictionary>
    </ResourceDictionary.ThemeDictionaries>
</ResourceDictionary>
